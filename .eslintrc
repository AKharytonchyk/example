{
    "env": {
        "es6": true,
        "browser": true
    },
    "plugins": [
        "babel",
        "prettier"
    ],
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "stylelint"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "globals": {
        "window": true,
        "document": true,
        "SpeechSynthesisUtterance":true
    }
}